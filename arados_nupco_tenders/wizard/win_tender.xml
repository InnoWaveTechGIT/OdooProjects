<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_wizard_form" model="ir.ui.view">
        <field name="name">custom.wizard.form</field>
        <field name="model">win.tender.wizard</field>
        <field name="arch" type="xml">
            <form string="NUPCO Order">
                <group>
                    <field name="po_number"/>
                    <field name="tender_name" readonly="1"/>
                    <field name="tender_id" readonly="1" invisible="1"/>
                </group>
                <footer>
                    <button name="action_add_details" string="Add" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
