<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_stock_lot_list_inherit156" model="ir.ui.view">
        <field name="name">stock.move.line.operations.list.inherit</field>
        <field name="model">stock.lot</field>
        <field name="inherit_id" ref="stock.view_production_lot_form"/>
        <field name="arch" type="xml">
            <xpath expr="//sheet[1]/group[1]/group[1]/field[1]" position="after">


                <field name="Production_warranty"/>
                <field name="tender_id"/>


            </xpath>
        </field>
    </record>

    <record id="view_stock_quant_list_inherit" model="ir.ui.view">
        <field name="name">stock.quant.operations.list.inherit</field>
        <field name="model">stock.quant</field>
        <field name="inherit_id" ref="stock.view_stock_quant_tree_simple"/>
        <field name="arch" type="xml">
            <xpath expr="//list" position="inside">

                <field name="tender_no"/>


            </xpath>
        </field>
    </record>


</odoo>
