<odoo>
    <data>
        <!-- Form View -->
        <record id="view_student_Track_form" model="ir.ui.view">
            <field name="name">student.track.report.form</field>
            <field name="model">student.track.report</field>
            <field name="arch" type="xml">
                <form string="Student Class">
                    <sheet>
                        <group>
                            <group>
                                <field name="student_id" />

                            </group>
                            <group>
                            </group>
                        </group>

                        <notebook>
                            <page string="Tracking">
                                <field name="track_ids">
                                    <tree editable="bottom">
                                        <field name="responseble_id" />
                                        <field name="date" />
                                        <field name='state' />
                                    </tree>
                                </field>
                            </page>

                        </notebook>

                    </sheet>
                </form>
            </field>
        </record>

        <!-- Tree View -->
        <record id="view_student_Track_tree" model="ir.ui.view">
            <field name="name">student.track.report.tree</field>
            <field name="model">student.track.report</field>
            <field name="arch" type="xml">
                <tree string="Student Class">
                    <field name="student_id" />

                </tree>
            </field>
        </record>

        <!-- Action -->
        <record id="action_student_Track" model="ir.actions.act_window">
            <field name="name">Student Track</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">student.track.report</field>
            <field name="view_mode">tree,form</field>
        </record>

        <!-- Menu -->
        <menuitem id="menu_student_Track_list" name="Student Track" parent="menu_student_report"
            action="action_student_Track" />
    </data>
</odoo>