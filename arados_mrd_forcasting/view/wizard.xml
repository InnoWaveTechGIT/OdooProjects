<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_scrap_wizard_form" model="ir.ui.view">
            <field name="name">scrap.wizard.form</field>
            <field name="model">add.month.wizard</field>
            <field name="arch" type="xml">
                <form string="Add Month Wizard">
                    <sheet>
                        <group>
                            <field name="quantity"/>
                        </group>
                        <footer>
                    <button name="add_months" string="Apply" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_vendor_wizaed_form" model="ir.ui.view">
            <field name="name">vendor.wizard.form</field>
            <field name="model">add.vendor.wizard</field>
            <field name="arch" type="xml">
                <form string="Add Vendor Wizard">
                    <sheet>
                        <group>
                            <field name="vendor_id"/>
                        </group>
                        <footer>
                    <button name="add_vendor" string="Apply" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="action_view_scrap_wizard_form" model="ir.actions.act_window">
            <field name="name">Scrap Wizard</field>
            <field name="res_model">add.month.wizard</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_scrap_wizard_form"/>
            <field name="target">new</field>
        </record>

        <record id="view_Goal_wizard_form" model="ir.ui.view">
            <field name="name">Goal.wizard.form</field>
            <field name="model">add.goal.wizard</field>
            <field name="arch" type="xml">
                <form string="Add Month Wizard">
                    <sheet>
                        <group>
                            <field name="quantity"/>
                        </group>
                        <footer>
                    <button name="add_goals" string="Apply" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="action_view_Goal_wizard_form" model="ir.actions.act_window">
            <field name="name">Scrap Wizard</field>
            <field name="res_model">add.goal.wizard</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_Goal_wizard_form"/>
            <field name="target">new</field>
        </record>
    </data>
</odoo>
