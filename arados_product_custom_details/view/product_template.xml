<odoo>

        <record id="product_template_vendor_inherit12325" model="ir.ui.view">
            <field name="name">product_template_vendor_inherit121234</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='barcode']" position="after">
                   <field name="mdma_code" />
                    <field name="MDMA_exp" />
                    <field name="SFGa_code" />
                  </xpath>
                <xpath expr="//field[@name='country_of_origin']" position="after">
                   <field name="sheif_life" />

                  </xpath>




            </field>
        </record>
</odoo>
