    <record model="ir.cron" id="ir_cron_scheduler_un_valid_section">

        <field name="name">calculate duration</field>

        <field name="model_id" ref="model_section" />

        <field name="state">code</field>

        <field name="code">model.cal_section_duration()</field>

        <field name="user_id" ref="base.user_root" />

        <field name="interval_number">1</field>

        <field name="interval_type">days</field>

        <field name="numbercall">-1</field>

    </record>
    <record model="ir.cron" id="get_data_for_session_status">

        <field name="name">Set the course unvalid</field>

        <field name="model_id" ref="model_session_status" />

        <field name="state">code</field>

        <field name="code">model.get_questin_and_grad()</field>

        <field name="user_id" ref="base.user_root" />

        <field name="interval_number">1</field>

        <field name="interval_type">days</field>

