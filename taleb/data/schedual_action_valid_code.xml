<?xml version="1.0" encoding="UTF-8"?>

<odoo>


    <record model="ir.cron" id="ir_cron_scheduler_un_valid_action">

        <field name="name">Set the code unvalid</field>

        <field name="model_id" ref="model_user_verification" />

        <field name="state">code</field>

        <field name="code">model.set_is_unvalid()</field>

        <field name="user_id" ref="base.user_root" />

        <field name="interval_number">10</field>

        <field name="interval_type">minutes</field>

        <field name="numbercall">-1</field>

    </record>



    <record model="ir.cron" id="ir_cron_scheduler_set_dacast_id">

        <field name="name">Set dacast id</field>

        <field name="model_id" ref="model_course_video" />

        <field name="state">code</field>

        <field name="code">model.set_dacast_id()</field>

        <field name="user_id" ref="base.user_root" />

        <field name="interval_number">10</field>

        <field name="interval_type">days</field>

        <field name="numbercall">-1</field>

    </record>

