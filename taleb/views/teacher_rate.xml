<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <record id="teacher_rate_list_view_taleb" model="ir.ui.view">

            <field name="name"> Teacher Rate List</field>

            <field name="model">teacher_rate</field>


            <field name="arch" type="xml">


                <tree>

                   
                    <field name="teacher_id" />
                    

                </tree>

            </field>

        </record>
        <record id="teacher_rate_form_view_taleb" model="ir.ui.view">

            <field name="name"> teacher rate Form</field>

            <field name="model">teacher_rate</field>


            <field name="arch" type="xml">


                <form string="Teacher Rate ">


                    <sheet>
                        <group>
                            <field name="teacher_id"></field>


                        </group>


                        <notebook>
                            <page name="" string="Rating">
                                <field name="rate_value">
                                    <tree>
                                        <field name="rataing" />
                                        <field name="comment" />
                                        <field name="date_of_create" />
                                        <field name="user_id" />
                                    </tree>

                                </field>

                            </page>

                        </notebook>


                    </sheet>

                </form>

            </field>

        </record>
        <record id="teacher_rate_window_taleb" model="ir.actions.act_window">

            <field name="name">Teacher rate</field>

            <field name="res_model">teacher_rate</field>

            <field name="view_mode">tree,form</field>

            <field name="context"></field>


            <!-- <field name="help" type="html">

                <p class="o_view_nocontent_smiling_face">Create your first user token Record! </p>

            </field> -->

        </record>

        <menuitem id="taleb_teacher_rates" name="Teacher rate" parent="base_taleb_user_rates"
            action="teacher_rate_window_taleb" />


           

    </data>


</odoo>