<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <record id="rate_list_view_taleb" model="ir.ui.view">

            <field name="name">Rate List</field>

            <field name="model">rate</field>


            <field name="arch" type="xml">


                <tree>

                   
                    <field name="course_id" />
                    

                </tree>

            </field>

        </record>
        <record id="rate_form_view_taleb" model="ir.ui.view">

            <field name="name">rate Form</field>

            <field name="model">rate</field>


            <field name="arch" type="xml">


                <form string="Rate">


                    <sheet>
                        <group>
                            <field name="course_id"></field>


                        </group>


                        <notebook>
                            <page name="" string="Rating">
                                <field name="rate_value">
                                    <tree>
                                        <field name="rataing" />
                                        <field name="comment" />
                                        <field name="date_of_create" />
                                        <field name="user_id" />
                                    </tree>

                                </field>

                            </page>

                        </notebook>


                    </sheet>

                </form>

            </field>

        </record>
        <record id="rate_window_taleb" model="ir.actions.act_window">

            <field name="name">User rate</field>

            <field name="res_model">rate</field>

            <field name="view_mode">tree,form</field>

            <field name="context"></field>


            <!-- <field name="help" type="html">

                <p class="o_view_nocontent_smiling_face">Create your first user token Record! </p>

            </field> -->

        </record>

        <menuitem id="base_taleb_user_rates" name="Rating" parent="teleb_root" sequence="9"/>
            <menuitem id="taleb_user_rates" name="Course rate" parent="base_taleb_user_rates"
            action="rate_window_taleb"/>


    </data>


</odoo>