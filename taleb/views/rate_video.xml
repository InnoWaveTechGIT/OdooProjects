<?xml version="1.0" encoding="UTF-8"?>
<odoo>
 <record id="teacher_rate_list_view_taleb" model="ir.ui.view">

            <field name="name"> Teacher Rate List</field>

            <field name="model">video.rate</field>


            <field name="arch" type="xml">


                <tree>

                   
                    <field name="video" />
                    

                </tree>

            </field>

        </record>
    <!-- Form View -->
    <record id="view_video_rate_form" model="ir.ui.view">
        <field name="name">video.rate.form</field>
        <field name="model">video.rate</field>
        <field name="arch" type="xml">
            <form string="Rate Video">
                <sheet>
                <group>
                           <field name="video"/>


                        </group>
                   
                    <notebook>
                            <page name="" string="Rating">
                                <field name="video_rate_value">
                                              
                                    <tree>
                                        <field name="rataing" />
                                        <field name="comment" />
                                        <field name="user_id" />
                                    </tree>

                                </field>

                            </page>

                        </notebook>
                </sheet>
            </form>
        </field>
    </record>
    
   

    <!-- Action -->
    <record id="action_video_rate" model="ir.actions.act_window">
        <field name="name">Rate Video</field>
        <field name="res_model">video.rate</field>
        <field name="view_mode">tree,form</field>
        <field name="context"/>
    </record>

    <!-- Menu Item -->
    
            <menuitem id="taleb_video_rates" name="Video rate" parent="base_taleb_user_rates"
            action="action_video_rate"/>
</odoo>