<?xml version="1.0"?>

<odoo>

    <data>


        <record id="user_article_list_view_taleb" model="ir.ui.view">

            <field name="name">User article List</field>

            <field name="model">daily.article</field>


            <field name="arch" type="xml">


                <tree>

                    <field name="title" />
                   


                </tree>

            </field>

        </record>


        <record id="user_daily_article_form_view_taleb" model="ir.ui.view">

            <field name="name">daily.article Form</field>

            <field name="model">daily.article</field>


            <field name="arch" type="xml">


                <form string="daily.article">


                    <sheet>


                        <group>


                            <group>

                                <field name="title" />
                                


                            </group>


                            <group>

                            </group>

                        </group>

                    </sheet>

                </form>

            </field>

        </record>


        <record id="daily_article_action_window_taleb" model="ir.actions.act_window">

            <field name="name">User Daily Article</field>

            <field name="res_model">daily.article</field>

            <field name="view_mode">tree,form</field>

            <field name="context"></field>


            <field name="help" type="html">

                <p class="o_view_nocontent_smiling_face">Create your first user token Record! </p>

            </field>

        </record>

        <menuitem id="taleb_user_daily_article" name="Daily Article" sequence="5" parent="teleb_root"
            action="daily_article_action_window_taleb" />

    </data>

</odoo>